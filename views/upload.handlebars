{{#if msgs}}
    <div class="alert alert-success">
        <strong>Success!</strong> You have logged in as "{{name}}"
    </div>
{{/if}}

{{#if err}}
	<div class="alert alert-danger">
        <strong>Sorry!</strong> You have crossed the file size limit
    </div>
{{/if}}
<h1>Upload your file</h1>
<br>
<form method="post" enctype="multipart/form-data" action="/upload">
    <input class="btn btn-primary" type="file" name="upfile"><br>
	<button class="btn btn-success" type="submit">Upload File</button>
</form>
